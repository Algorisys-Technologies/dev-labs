generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model account {
  id    Int     @id @default(autoincrement())
  email String? @unique @db.VarChar(255)
}

model enron_emaildataset {
  id                     Int                      @id @default(autoincrement())
  from                   String?                  @db.VarChar(100)
  to                     String?
  date                   DateTime?                @db.Timestamp(6)
  subject                String?
  body_text              String?
  status                 String?                  @default("unseen") @db.VarChar(50)
  createdat              DateTime?                @default(now()) @db.Timestamp(6)
  emailid                Int?
  attachments            Json?
  body_embedding_data    body_embedding_data[]    @ignore
  subject_embedding_data subject_embedding_data[] @ignore

  @@unique([subject, date, to, from, body_text])
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model body_embedding_data {
  id                  Int?
  body_embeddings_bin Bytes?
  enron_emaildataset  enron_emaildataset? @relation(fields: [id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model subject_embedding_data {
  id                     Int?
  subject_embeddings_bin Bytes?
  enron_emaildataset     enron_emaildataset? @relation(fields: [id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@ignore
}
