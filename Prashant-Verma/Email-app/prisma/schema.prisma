datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Email {
  id        Int      @id @default(autoincrement())
  from      String
  to        String
  subject   String?
  date      DateTime
  bodyText  String?
  createdAt DateTime @default(now())
}
